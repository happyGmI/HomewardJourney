<!--index.wxml-->
<image class="title-image" src="../../images/1637412558059.png" mode="widthFix"></image>
<view class="container">
  <view class="main-title"><text class="main-title-text">{{title}}</text></view>
  <view class="lost-info">
    
    <view class="title"><text class="title-text">丢失物品详情\n</text></view>
    <view class="info"><text class="info-text">捡到的地点：{{place}}</text></view>
    <view class="info"><text class="info-text">捡到的时间：{{time}}</text></view>
    <view class="info"><text class="info-text">具体细节信息：{{detail}}</text></view>
    <image class="info-image"src="{{url}}" mode="widthFix"></image>
  </view>
  <view class="retrieve-contact-info">
    <view class="title"><text class="title-text">找回物品联系方式\n</text></view>
    <view class="info1">联系人：{{contacts}}</view>
    <view class="info2" bindtap="contact">联系方式：{{Tel}}</view>
  </view>
</view> 